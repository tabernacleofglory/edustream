
{
  "hosting": [
    {
      "site": "edustream-5t6z4",
      "public": ".next",
      "ignore": [
        "firebase-debug.log",
        "firebase-debug.*.log",
        "firestore.rules"
      ],
      "headers": [
        {
          "source": "**",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "no-cache, no-store, must-revalidate"
            }
          ]
        }
      ],
      "app": {
        "backend": "studio"
      }
    }
  ],
  "functions": {
    "source": "src/functions",
    "runtime": "nodejs20"
  },
  "firestore": {
    "rules": "firestore.rules"
  }
}
